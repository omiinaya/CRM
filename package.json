{
  "name": "crm-app",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "scripts": {
    "server": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn client\"",
    "client": "cd client && yarn start",
    "install": "cd client && yarn install",
    "dev": "concurrently \"yarn server\"",
    "build": "cd client && yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn start:prod || yarn start:dev",
    "postinstall": "cd client/src && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omiinaya/b3"
  },
  "author": "VP",
  "license": "No-License",
  "dependencies": {
    "axios": "^0.21.4",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "node-fetch": "^2.6.5",
    "node-pre-gyp": "^0.17.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^17.0.2",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
    "sass": "^1.42.1",
    "sequelize": "^5.22.4",
    "sequelize-auto": "^0.6.7",
    "serve": "^11.3.2"
  },
  "bugs": {
    "url": "https://github.com/omiinaya/b3/issues"
  },
  "homepage": "https://github.com/omiinaya/",
